Host *
  IgnoreUnknown UseKeychain
  UseKeychain yes

Host uptermd.upterm.dev
  PubkeyAcceptedAlgorithms +ssh-rsa
  HostkeyAlgorithms +ssh-rsa

# Personal GitHub account
Host github.com
  # HostName github.com
  Hostname ssh.github.com
  Port 443
  User git
  AddKeysToAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes
  IdentityFile ~/.ssh/shahzadlone@gmail.com_ed25519

